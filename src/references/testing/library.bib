doi = {10.1002/mrm.27742},
file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Wilson et al. - 2019 - Methodological consensus on clinical proton MRS of the brain Review and recommendations.pdf:pdf},
