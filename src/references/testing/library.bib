month = {aug},
number = {3},
pages = {641--653},
pmid = {16787421},
title = {{The glutamate/GABA-glutamine cycle: Aspects of transport, neurotransmitter homeostasis and ammonia transfer}},
url = {http://doi.wiley.com/10.1111/j.1471-4159.2006.03913.x},
volume = {98},
