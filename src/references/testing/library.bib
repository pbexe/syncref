issn = {15071367},
journal = {Reports Pract. Oncol. Radiother.},
keywords = {MRI,Stereotactic radiosurgery planning,Tumor},
number = {1},
pages = {12--19},
publisher = {Wielkopolskie Centrum Onkologii},
title = {{Optimizing MRI sequences and images for MRI-based stereotactic radiosurgery treatment planning}},
url = {https://doi.org/10.1016/j.rpor.2018.09.010},
volume = {24},
year = {2019}
}
