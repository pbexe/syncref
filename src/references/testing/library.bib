@article{Slotboom1994,
abstract = {The effects of solvent-suppression, sinc-Gauss, self-refocussed, and$\backslash$n2D selective RF pulses on J-coupled spin systems have been investigated$\backslash$nby means of computer simulations. Depending on the pulse time, coupling$\backslash$nconstants, and chemical shifts, nonnegligible amounts of unwanted$\backslash$ncoherence may be generated, resulting in signal loss and deteriorated$\backslash$nslice profiles. A time-inverted self-refocused frequency-selective$\backslash$npulse is useful for frequency-selective multiple-quantum generation$\backslash$nand spectral editing. Volume-selective pulses with bandwidths in$\backslash$nthe order of the chemical-shift range may lead for the PRESS experiment$\backslash$nto severe signal cancellation. The SADLOVE localization sequence$\backslash$ndoes not have this effect. It is shown that except for multiple-pulse$\backslash$nlocalization sequences, with RF bandwidths on the order of the chemical-shift$\backslash$nrange, the effects of J coupling can be neglected for in vivo use.$\backslash$n},
