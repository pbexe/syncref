@article{DelliPizzi2016,
abstract = {The ventromedial prefrontal cortex (vmPFC) plays a key role in emotion processing and regulation. vmPFC dysfunction may lead to disinhibition of amygdala causing high anxiety levels. $\gamma$-Aminobutyric acid (GABA) inter-neurons within vmPFC shape the information flow to amygdala. Thus, we hypothesize that GABA content within vmPFC could be relevant to trait anxiety. Forty-three healthy volunteers aged between 20 and 88 years were assessed for trait anxiety with the Subscale-2 of the State-Trait-Anxiety Inventory (STAI-Y2) and were studied with proton magnetic resonance spectroscopy ((1)H-MRS) to investigate GABA and Glx (glutamate+glutamine) contents within vmPFC. Total creatine (tCr) was used as internal reference. Partial correlations assessed the association between metabolite levels and STAI-Y2 scores, removing the effect of possible nuisance factors including age, educational level, volumes of grey (GM) and white matter (WM) within MRS voxel. We observed a positive relationship between GABA/tCr and STAI-Y2 scores. No significant relationships were found between Glx/tCr and STAI-Y2 and between tCr/water and STAI-Y2. No differences were found between males and females as regards to age, STAI-Y2, GABA/tCr, Glx/tCr, tCr/water, GM and WM volumes. We suggest a close relationship between GABA content within vmPFC and trait anxiety providing new insights in the physiology of emotional brain.},
author = {{Delli Pizzi}, Stefano and Padulo, Caterina and Brancucci, Alfredo and Bubbico, Giovanna and Edden, Richard A. and Ferretti, Antonio and Franciotti, Raffaella and Manippa, Valerio and Marzoli, Daniele and Onofrj, Marco and Sepede, Gianna and Tartaro, Armando and Tommasi, Luca and Puglisi-Allegra, Stefano and Bonanni, Laura},
doi = {10.1093/scan/nsv155},
file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Delli Pizzi et al. - 2016 - GABA content within the ventromedial prefrontal cortex is related to trait anxiety.pdf:pdf},
issn = {17495024},
journal = {Soc. Cogn. Affect. Neurosci.},
keywords = {Anxiety,Glutamate,Ventromedial prefrontal cortex,$\gamma$-aminobutyric acid},
number = {5},
pages = {758--766},
title = {{GABA content within the ventromedial prefrontal cortex is related to trait anxiety}},
volume = {11},
year = {2016}
}
