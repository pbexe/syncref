journal = {Proc. 22nd Int. Conf. Mach. Learn.  - ICML '05},
keywords = {Gaussian processes,active learning,experimental design,sensor networks,spatial learning},
pages = {265--272},
title = {{Near-optimal sensor placements in Gaussian processes}},
