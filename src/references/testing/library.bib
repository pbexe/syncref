file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lundervold, Lundervold - 2019 - An overview of deep learning in medical imaging focusing on MRI.pdf:pdf},
issn = {09393889},
journal = {Z. Med. Phys.},
keywords = {Deep learning,MRI,Machine learning,Medical imaging},
month = {may},
number = {2},
pages = {102--127},
title = {{An overview of deep learning in medical imaging focusing on MRI}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0939388918301181},
volume = {29},
