file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fardanesh et al. - 2019 - Proton MR spectroscopy in the breast Technical innovations and clinical applications.pdf:pdf},
issn = {15222586},
journal = {J. Magn. Reson. Imaging},
keywords = {Proton MR spectroscopy,breast cancer metabolism,choline metabolites,contrast-enhanced MRI,lipid metabolites},
title = {{Proton MR spectroscopy in the breast: Technical innovations and clinical applications}},
year = {2019}
}
@article{Bull2011,
abstract = {In the efficient global optimization problem, we minimize an$\backslash$nunknown function f, using as fewobservations f(x) as possible. It$\backslash$ncan be considered a continuum-armed-bandit problem, with$\backslash$nnoiseless data, and simple regret. Expected-improvement$\backslash$nalgorithms are perhaps the most popular methods for solving the$\backslash$nproblem; in this paper, we provide theoretical results on their$\backslash$nasymptotic behaviour. Implementing these algorithms requires a$\backslash$nchoice of Gaussian-process prior, which determines an associated$\backslash$nspace of functions, its reproducing-kernel Hilbert space (RKHS).$\backslash$nWhen the prior is fixed, expected improvement is known to$\backslash$nconverge on the minimum of any function in its RKHS. We provide$\backslash$nconvergence rates for this procedure, optimal for functions of$\backslash$nlow smoothness, and describe a modified algorithm attaining$\backslash$noptimal rates for smoother functions. In practice, however,$\backslash$npriors are typically estimated sequentially from the data. For$\backslash$nstandard estimators, we show this procedure may never find the$\backslash$nminimum of f. We then propose alternative estimators, chosen to$\backslash$nminimize the constants in the rate of convergence, and show these$\backslash$nestimators retain the convergence rates of a fixed$\backslash$nprior.$\backslash$textbackslash{\{}{\}}n},
archivePrefix = {arXiv},
arxivId = {arXiv:1101.3501v3},
author = {Bull, Adam D},
eprint = {arXiv:1101.3501v3},
isbn = {1532-4435},
issn = {1532-4435},
journal = {J. Mach. Learn. Res.},
keywords = {Bayesian optimization; convergence rate; expected},
pages = {2879--2904},
title = {{Convergence Rates of Efficient Global Optimization Algorithms}},
url = {http://arxiv.org/abs/1101.3501},
volume = {12},
year = {2011}
}
