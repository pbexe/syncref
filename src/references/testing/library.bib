pages = {15},
title = {{Markovianity and non-Markovianity in quantum and classical systems}},
url = {http://arxiv.org/abs/1106.0138 http://stacks.iop.org/1367-2630/13/i=9/a=093004?key=crossref.96edc459d434d9790ce036aa94cf937e},
