}
@inproceedings{Lizotte2007,
abstract = {Gait optimization is a basic yet challenging problem$\backslash$nfor both quadrupedal and bipedal robots. Although$\backslash$ntechniques for automating the process exist,$\backslash$nmost involve local function optimization procedures$\backslash$nthat suffer from three key drawbacks. Local$\backslash$noptimization techniques are naturally plagued$\backslash$nby local optima, make no use of the expensive gait$\backslash$nevaluations once a local step is taken, and do not$\backslash$nexplicitly model noise in gait evaluation. These$\backslash$ndrawbacks increase the need for a large number$\backslash$nof gait evaluations, making optimization slow, data$\backslash$ninefficient, and manually intensive. We present$\backslash$na Bayesian approach based on Gaussian process$\backslash$nregression that addresses all three drawbacks. It$\backslash$nuses a global search strategy based on a posterior$\backslash$nmodel inferred from all of the individual noisy$\backslash$nevaluations. We demonstrate the technique on a$\backslash$nquadruped robot, using it to optimize two different$\backslash$ncriteria: speed and smoothness. We show in both$\backslash$ncases our technique requires dramatically fewer$\backslash$ngait evaluations than state-of-the-art local gradient$\backslash$napproaches.},
author = {Lizotte, Daniel and Wang, Tao and Bowling, Michael and Schuurmans, Dale},
booktitle = {IJCAI Int. Jt. Conf. Artif. Intell.},
issn = {10450823},
keywords = {control-learning,robotics},
pages = {944--949},
title = {{Automatic gait optimization with Gaussian process regression}},
url = {http://www.aaai.org/Papers/IJCAI/2007/IJCAI07-152.pdf},
year = {2007}
}
@article{Osorio-Garcia,
abstract = {Quantification of magnetic resonance spectroscopy (MRS) signals is$\backslash$nrequired for providing metabolite concentrations of the tissue under$\backslash$ninvestigation. For estimating these concentrations several biochemical$\backslash$nand acquisition conditions need to be taken into account. It is still$\backslash$na challenge to obtain reliable concentrations, as experimental conditions$\backslash$nmay have a detrimental effect on the spectral quality. The lineshape$\backslash$nof MRS signals is affected, for instance, by inhomogeneities of the$\backslash$nstatic magnetic field arising from imperfect shimming and tissue$\backslash$nheterogeneities. To handle this type of distortions, we propose an$\backslash$nextension of the self-deconvolution method, where a common lineshape$\backslash$nis estimated and a robust method with local regression is included$\backslash$nto improve the smoothing of the estimated damping (or lineshape)$\backslash$nfunction. This common lineshape is imposed in the metabolite quantification$\backslash$nmethod and the spectral parameters (amplitude, frequency, damping$\backslash$nand phase corrections) are obtained via nonlinear least squares.$\backslash$nIn this study, we considered distorted simulated, in vitro and in$\backslash$nvivo rat brain signals which were lineshape corrected and quantitative$\backslash$nresults were compared in all three cases. Copyright {\{}$\backslash$copyright{\}} 2011 John$\backslash$nWiley {\{}{\&}{\}} Sons, Ltd.},
