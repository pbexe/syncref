isbn = {1090-7807},
issn = {10907807},
journal = {J. Magn. Reson.},
keywords = {BEBOP,Broadband excitation,Optimal control theory},
month = {jul},
number = {1},
pages = {8--15},
pmid = {12852902},
title = {{Application of optimal control theory to the design of broadband excitation pulses for high-resolution NMR}},
