title = {{An automated quantitation of short echo time MRS spectra in an open source software environment: AQSES}},
url = {http://www.dotynmr.com/PDF/SA{\_}RFCoils2b.pdf http://doi.wiley.com/10.1002/nbm.1112},
volume = {20},
