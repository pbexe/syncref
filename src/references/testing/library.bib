issn = {0129-055X},
journal = {Rev. Math. Phys.},
keywords = {49-02,49r50,53-02,53cxx,65kxx,81v70,algorithms,constrained optimization in quantum,double-bracket flows,low-,mathematics subject classification 2010,quantum control,rank approximation of tensors,riemannian gradient flows and,riemannian optimization,systems,tensor svd},
month = {feb},
