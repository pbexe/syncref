isbn = {9783030009274},
issn = {16113349},
journal = {Lect. Notes Comput. Sci. (including Subser. Lect. Notes Artif. Intell. Lect. Notes Bioinformatics)},
keywords = {Convolutional Neural Networks,Deep learning,Magnetic Resonance Spectroscopy (MRS),Metabolites,Parameter estimation,Short echo time,Time series regression},
month = {jun},
pages = {467--475},
title = {{Magnetic Resonance Spectroscopy Quantification using Deep Learning}},
