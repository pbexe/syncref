issn = {1066033X},
journal = {IEEE Control Syst.},
month = {jun},
number = {3},
pages = {26--33},
title = {{Optimal Control-1950 to 1985}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=506395},
volume = {16},
year = {1996}
}
