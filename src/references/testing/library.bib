@article{Auger2010,
abstract = {This paper analyses extensions of No-Free-Lunch (NFL) theorems to$\backslash$r$\backslash$ncountably inﬁnite and uncountable inﬁnite domains and investigates the$\backslash$r$\backslash$ndesign of optimal optimization algorithms.$\backslash$r$\backslash$n    The original NFL theorem due to Wolpert and Macready states that,$\backslash$r$\backslash$nfor ﬁnite search domains, all search heuristics have the same performance$\backslash$r$\backslash$nwhen averaged over the uniform distribution over all possible functions.$\backslash$r$\backslash$nFor inﬁnite domains the extension of the concept of distribution over all$\backslash$r$\backslash$npossible functions involves measurability issues and stochastic process the-$\backslash$r$\backslash$nory. For countably inﬁnite domains, we prove that the natural extension$\backslash$r$\backslash$nof NFL theorems, for the current formalization of probability, does not$\backslash$r$\backslash$nhold, but that a weaker form of NFL does hold, by stating the existence$\backslash$r$\backslash$nof non-trivial distributions of ﬁtness leading to equal performances for all$\backslash$r$\backslash$nsearch heuristics. Our main result is that for continuous domains, NFL$\backslash$r$\backslash$ndoes not hold. This free-lunch theorem is based on the formalization of$\backslash$r$\backslash$nthe concept of random ﬁtness functions by means of random ﬁelds.$\backslash$r$\backslash$n    We also consider the design of optimal optimization algorithms for a$\backslash$r$\backslash$ngiven random ﬁeld, in a black-box setting, namely, a complexity measure$\backslash$r$\backslash$nbased solely on the number of requests to the ﬁtness function. We derive$\backslash$r$\backslash$nan optimal algorithm based on Bellman's decomposition principle, for$\backslash$r$\backslash$na given number of iterates and a given distribution of ﬁtness functions.$\backslash$r$\backslash$nWe also approximate this algorithm thanks to a Monte-Carlo planning$\backslash$r$\backslash$nalgorithm close to the UCT (Upper Conﬁdence Trees) algorithm, and$\backslash$r$\backslash$nprovide experimental results.},
author = {Auger, Anne and Teytaud, Olivier},
doi = {10.1007/s00453-008-9244-5},
isbn = {0178-4617},
issn = {01784617},
journal = {Algorithmica (New York)},
keywords = {Bandit-based Monte-Carlo planning,Complexity,Dynamic programming,Expensive optimization,Kolmogorov's extension theorem,No-Free-Lunch},
month = {may},
number = {1},
pages = {121--146},
title = {{Continuous lunches are free plus the design of optimal optimization algorithms}},
volume = {57},
year = {2010}
}
@article{Jenkins2019,
abstract = {Purpose: Many tools exist for quantifying magnetic resonance spectroscopy (MRS) data. Literature comparing them is sparse but indicates potential methodological bias. We benchmark MRS analysis tools to elucidate this. Methods: Four series of phantom experiments, including both solutions and tissue-mimicking gels, with constant concentrations of NAA, Creatine, Glutamine and Glutamate, and iteratively increased concentrations of GABA are performed. MEGA-PRESS spectra are acquired and quantified with several state-of-the-art MRS analysis tools (LCModel, TARQUIN, JMRUI, GANNET) and in-house code (LWFIT). GABA-to-NAA ratios for reported metabolite amplitudes are compared to the ground truth of known concentration ratios. Overall estimation accuracy is assessed by linear fits of reported vs. actual ratios and coefficients of determination. Simulations further elucidate the experimental results. Results: Significant differences in reported GABA-to-NAA amplitude ratios are observed. TARQUIN consistently overestimates, while most tools underestimate ratios to varying degrees compared to the ground truth. Underestimationdue to reduced editing efficiency is predicted by simulations. LCModel performs comparatively well for well-resolved solution spectra but struggles for intentionally miscalibrated spectra and gel spectra mimicking in-vivo conditions. GANNET shows better consistency and robustness to calibration errors but greater underestimation related to how the 3 ppm GABA peak is fitted. Surprisingly, simple peak integration with minimal pre-processing yields the most consistent and accurate results compared to the ground truth. Conclusions: A methodological dependence is observed not only in the quantification results for individual spectra, but in GABA-to-NAA gradients across experimental series, systematic offsets and coefficients of determination.},
archivePrefix = {arXiv},
arxivId = {1909.02163},
author = {Jenkins, C. and Chandler, M. and Langbein, F. C. and Shermer, S. M.},
eprint = {1909.02163},
file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chandler et al. - 2019 - MRSNet Metabolite Quantification from Edited Magnetic Resonance Spectra With Convolutional Neural Networks(2).pdf:pdf},
month = {sep},
number = {Mc},
pages = {1--23},
title = {{Seeking Ground Truth for GABA Quantification by Edited Magnetic Resonance Spectroscopy:Comparative Analysis of TARQUIN, LCModel, JMRUI and GANNET}},
url = {http://arxiv.org/abs/1909.02163},
year = {2019}
}
@article{Saleh2019,
abstract = {Spectral editing allows direct measurement of low-concentration metabolites, such as GABA, glutathione (GSH) and lactate (Lac), relevant for understanding brain (patho)physiology. The most widely used spectral editing technique is MEGA-PRESS, which has been diversely implemented across research sites and vendors, resulting in variations in the final resolved edited signal. In this paper, we describe an effort to develop a new universal MEGA-PRESS sequence with HERMES functionality for the major MR vendor platforms with standardized RF pulse shapes, durations, amplitudes and timings. New RF pulses were generated for the universal sequence. Phantom experiments were conducted on Philips, Siemens, GE and Canon 3 T MRI scanners using 32-channel head coils. In vivo experiments were performed on the same six subjects on Philips and Siemens scanners, and on two additional subjects, one on GE and one on Canon scanners. On each platform, edited MRS experiments were conducted with the vendor-native and universal MEGA-PRESS sequences for GABA (TE = 68 ms) and Lac editing (TE = 140 ms). Additionally, HERMES for GABA and GSH was performed using the universal sequence at TE = 80 ms. The universal sequence improves inter-vendor similarity of GABA-edited and Lac-edited MEGA-PRESS spectra. The universal HERMES sequence yields both GABA- and GSH-edited spectra with negligible levels of crosstalk on all four platforms, and with strong agreement among vendors for both edited spectra. In vivo GABA+/Cr, Lac/Cr and GSH/Cr ratios showed relatively low variation between scanners using the universal sequence. In conclusion, phantom and in vivo experiments demonstrate successful implementation of the universal sequence across all four major vendors, allowing editing of several metabolites across a range of TEs.},
author = {Saleh, Muhammad G. and Rimbault, Daniel and Mikkelsen, Mark and Oeltzschner, Georg and Wang, Anna M. and Jiang, Dengrong and Alhamud, Ali and Near, Jamie and Sch{\"{a}}r, Michael and Noeske, Ralph and Murdoch, James B. and Ersland, Lars and Craven, Alexander R. and Dwyer, Gerard Eric and Gr{\"{u}}ner, Eli Renate and Pan, Li and Ahn, Sinyeob and Edden, Richard A.E.},
doi = {10.1016/j.neuroimage.2019.01.056},
file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Saleh et al. - 2019 - Multi-vendor standardized sequence for edited magnetic resonance spectroscopy.pdf:pdf},
issn = {10959572},
journal = {Neuroimage},
keywords = {Edited MRS,HERMES,MEGA-PRESS,Magnetic resonance spectroscopy,Standardization},
number = {January},
pages = {425--431},
title = {{Multi-vendor standardized sequence for edited magnetic resonance spectroscopy}},
volume = {189},
year = {2019}
}
