isbn = {0740-3194},
issn = {07403194},
journal = {Magn. Reson. Med.},
keywords = {Proton MRS,glutamate,glutamine,hepatic encephalopathy},
month = {apr},
number = {4},
pages = {615--618},
pmid = {9094085},
title = {{Detection of glutamate/glutamine resonances by 1H magnetic resonance spectroscopy at 0.5 Tesla}},
