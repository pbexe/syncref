pages = {201--208},
title = {{Comparison of single-shot localization methods (steam and press) forIn vivo proton NMR spectroscopy}},
url = {http://doi.wiley.com/10.1002/nbm.1940020506},
volume = {2},
year = {1989}
}
@article{Lloyd2000,
abstract = {In the conventional picture of quantum feedback control, sensors perform$\backslash$nmeasurements on the system, a classical controller processes the$\backslash$nresults of the measurements, and actuators supply semiclassical potentials$\backslash$nto alter the behavior of the quantum system. In this picture, the$\backslash$nsensors tend to destroy coherence in the process of making measurements,$\backslash$nand although the controller can use the actuators to act coherently$\backslash$non the quantum system, it is processing and feeding back classical$\backslash$ninformation. This paper proposes an alternative method for quantum$\backslash$nfeedback control, in which the sensors, controller, and actuators$\backslash$nare quantum systems that interact coherently with the system to be$\backslash$ncontrolled. In this picture, the controller gets, processes, and$\backslash$nfeeds back quantum information. Controllers that operate using such$\backslash$nquantum feedback loops can perform tasks such as entanglement transfer$\backslash$nthat are not possible using classical feedback. Necessary and sufficient$\backslash$nconditions are presented for Hamiltonian quantum systems to be controllable$\backslash$nand observable using both classical and quantum feedback.},
author = {Lloyd, Seth},
doi = {10.1103/PhysRevA.62.022108},
file = {:home/max/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Lloyd - 2000 - Coherent quantum feedback.pdf:pdf},
issn = {1050-2947},
journal = {Phys. Rev. A},
month = {jul},
number = {2},
pages = {1--12},
title = {{Coherent quantum feedback}},
url = {http://link.aps.org/doi/10.1103/PhysRevA.62.022108},
volume = {62},
year = {2000}
}
