isbn = {9781631178764},
journal = {Syst. Theory Perspect. Appl. Dev.},
keywords = {49k15,90c30,93c15,93c95,a ms subject classification,direct methods,indirect methods,numerical software,optimal control,rubella},
month = {jan},
